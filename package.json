{
  "name": "PRS",
  "productName": "PRS",
  "description": "Product Report System for Beauty Enterprises",
  "version": "0.0.1",
  "private": true,
  "author": "Laurence Willis <laurence.e.willis@gmail.com>",
  "copyright": "Â©2018-2020 Beauty Enterprises Inc.",
  "homepage": "",
  "main": "app/background.js",
  "build": {
    "appId": "com.example.electron-boilerplate",
    "files": [
      "app/**/*",
      "data/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "pree2e": "webpack --config=build/webpack.app.config.js --env=test --display=none && webpack --config=build/webpack.e2e.config.js --env=test --display=none",
    "e2e": "mocha temp/e2e.js --require source-map-support/register",
    "test": "npm run unit && npm run e2e",
    "start": "node build/start.js",
    "release": "webpack --config=build/webpack.app.config.js --env=production && electron-builder"
  },
  "dependencies": {
    "csv-parser": "^2.2.0",
    "fs-jetpack": "^2.1.0",
    "json2csv": "^4.5.3",
    "nedb": "^1.8.0",
    "ramda": "^0.25.0",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "chai": "^4.1.0",
    "css-loader": "^1.0.0",
    "electron": "^9.4.0",
    "electron-builder": "^20.39.0",
    "electron-mocha": "^9.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mocha": "^8.1.3",
    "source-map-support": "^0.5.6",
    "spectron": "^12.0.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}
